  "devDependencies": {
    "@iconify/vue": "^4.0.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@tailwindcss/typography": "^0.5.8",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mapbox-gl": "^2.7.9",
    "@types/node": "^18.11.9",
