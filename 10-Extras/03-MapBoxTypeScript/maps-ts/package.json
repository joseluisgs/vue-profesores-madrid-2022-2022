    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^3.2.4",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
